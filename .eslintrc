{
  "extends": "airbnb-base",
  "env": {
    "jasmine": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "max-len": [2, 80, 4, {"ignorePattern": "^import\\s.*\\sfrom\\s'.*';$|^  from\\s.*$"}],
    "import/export": 2,
    "import/imports-first": [2, {"absolute-first": true}],
    "import/named": 0,
    "import/namespace": 2,
    "import/no-duplicates": 2,
    "import/no-named-as-default": 2,
    "import/no-unresolved": 0,
    "import/no-mutable-exports": 0,
    "object-shorthand": 0,
    "quote-props": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "camelcase": 0,
    "padded-blocks": 0,
    "eqeqeq": 0,
    "prefer-template": 0,
    "consistent-return": 0,
    "func-names": 0,
    "no-else-return": 0,
    "one-var-declaration-per-line": 0,
    "radix": 0,
    "indent": 0,
    "prefer-rest-params": 0,
    "no-param-reassign": 0,
    "one-var": 0,
    "no-use-before-define": 0,
    "prefer-arrow-callback": 0,
    "no-shadow": 0,
    "arrow-body-style": 0,
    "no-unused-expressions": 0,
    "no-sequences": 0,
    "dot-notation": 0,
    "no-array-constructor": 0,
    "new-cap": 0,
    "no-return-assign": 0,
    "brace-style": 0,
    "block-scoped-var": 0,
    "curly": 0,
    "prefer-const": 0,
    "no-throw-literal": 0,
    "no-unneeded-ternary": 0,
    "no-multiple-empty-lines": 0,
    "no-new-wrappers": 0,
    "wrap-iife": 0,
    "no-new": 0,
    "guard-for-in": 0,
    "no-new-object": 0,
    "no-caller": 0,
    "no-nested-ternary": 0,
    "default-case": 0,
    "no-unreachable": 0,
    "no-alert": 0,
    "no-eval": 0,
    "no-extend-native": 0,
    "no-underscore-dangle": 0,
    "newline-per-chained-call": 0,
    "no-restricted-syntax": 0,
    "no-console": 0,
    "no-plusplus": 0
  }
}
